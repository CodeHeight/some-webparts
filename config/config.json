{
  "version": "2.0",
  "bundles": {
    "weather-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/weather/WeatherWebPart.js",
          "manifest": "./src/webparts/weather/WeatherWebPart.manifest.json"
        }
      ]
    },
    "image-magnifier-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/imageMagnifier/ImageMagnifierWebPart.js",
          "manifest": "./src/webparts/imageMagnifier/ImageMagnifierWebPart.manifest.json"
        }
      ]
    },
    "todo-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/todo/TodoWebPart.js",
          "manifest": "./src/webparts/todo/TodoWebPart.manifest.json"
        }
      ]
    },
    "poll-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/poll/PollWebPart.js",
          "manifest": "./src/webparts/poll/PollWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "WeatherWebPartStrings": "lib/webparts/weather/loc/{locale}.js",
    "ImageMagnifierWebPartStrings": "lib/webparts/imageMagnifier/loc/{locale}.js",
    "TodoWebPartStrings": "lib/webparts/todo/loc/{locale}.js",
    "PollWebPartStrings": "lib/webparts/poll/loc/{locale}.js"
  }
}
